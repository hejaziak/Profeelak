<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Profeelak</title>

<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<link rel="stylesheet" href='/stylesheets/style.css'>
<link href='//fonts.googleapis.com/css?family=Audiowide' rel='stylesheet'>
<link href='//fonts.googleapis.com/css?family=Damion' rel='stylesheet'>

</head>
<body>

<nav class="navbar navbar-inverse navbar-static-top" role="navigation">
    <div class="container">
      <div class="navbar-header">
 
        <img class="navbar-left " alt="Brand" src="/logoI.png">
        <a class="navbar-brand profeelak-header" href="/page/1">Profeelak</a>
  </div>
      <ul class="nav navbar-nav navbar-right">
        <% if (currentUser) { %>
          <li>
            <a href="/users/<%= currentUser.username %>">
              <%= currentUser.name() %>
            </a>
          </li>
          <li><a href="/logout">Log out</a></li>
        <% } else { %>
          <li><a href="/login">Log in</a></li>
          <li><a href="/signup">Sign up</a></li>
        <% } %>
  </ul> </div>
</nav>
<div class="container">
  <% errors.forEach(function(error) { %>
    <div class="alert alert-danger" role="alert">
      <%= error %>
    </div>
<% }) %>
  <% infos.forEach(function(info) { %>
    <div class="alert alert-info" role="alert">
      <%= info %>
    </div>
<% }) %>
